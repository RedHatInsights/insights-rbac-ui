{
  "compilerOptions": {
    "outDir": "./build/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "esnext",
    "target": "esnext",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "removeComments": false,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["src/redux/store.d.ts"]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
, "src/presentational-components/shared/AppLink.js", "src/presentational-components/shared/AppLink.js"  ]
}
