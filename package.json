    "@patternfly/react-table": "^5.1.1",
    "@patternfly/react-tokens": "^5.1.1",
    "@redhat-cloud-services/access-requests-frontend": "^1.2.11",
    "@redhat-cloud-services/cost-management-client": "^1.0.112",
    "@redhat-cloud-services/frontend-components": "^4.2.3",
    "@redhat-cloud-services/frontend-components-notifications": "^4.1.0",
    "@redhat-cloud-services/frontend-components-utilities": "^4.0.2",
    "@redhat-cloud-services/host-inventory-client": "^4.0.1",
    "@redhat-cloud-services/javascript-clients-shared": "^2.0.0",
    "@redhat-cloud-services/rbac-client": "^4.0.2",
