  "scripts": {
    "prepare": "husky",
    "build": "fec build",
    "test": "sh -c 'npm run test:web && npm run test:cli'",
    "test:web": "TZ=UTC vitest run",
    "test:watch": "TZ=UTC vitest",
    "test:ui": "TZ=UTC vitest --ui",
    "test:e2e": "npx playwright test --config=e2e/playwright.config.ts",
    "e2e:ci:setup": "test -n \"$CI\" && for f in e2e/auth/.env.*.ci; do envsubst < \"$f\" > \"${f%.ci}\"; done || echo 'Skipped: CI env var not set (run in CI only)'",
    "e2e:v1:seed": "node scripts/run-with-env.js e2e/auth/.env.v1-admin npm run cli -- seed --file e2e/fixtures/seed-v1.json --prefix \"${TEST_PREFIX_V1}\" --json --output e2e/seed-map-v1.json",
    "e2e:v1:cleanup": "node scripts/run-with-env.js e2e/auth/.env.v1-admin npm run cli -- cleanup --prefix \"${TEST_PREFIX_V1}\"",
    "e2e:v1:test": "TEST_VERSION=v1 npx playwright test --config=e2e/playwright.config.ts --project=v1-admin --project=v1-userviewer --project=v1-readonly",
    "e2e:v1:test:admin": "TEST_VERSION=v1 npx playwright test --config=e2e/playwright.config.ts --project=v1-admin",
    "e2e:v1:test:userviewer": "TEST_VERSION=v1 npx playwright test --config=e2e/playwright.config.ts --project=v1-userviewer",
    "e2e:v1:test:readonly": "TEST_VERSION=v1 npx playwright test --config=e2e/playwright.config.ts --project=v1-readonly",
    "e2e:v1": "npm run e2e:v1:seed && npm run e2e:v1:test; npm run e2e:v1:cleanup",
    "e2e:v1:admin": "npm run e2e:v1:seed && npm run e2e:v1:test:admin; npm run e2e:v1:cleanup",
    "e2e:v1:userviewer": "npm run e2e:v1:seed && npm run e2e:v1:test:userviewer; npm run e2e:v1:cleanup",
    "e2e:v1:readonly": "npm run e2e:v1:seed && npm run e2e:v1:test:readonly; npm run e2e:v1:cleanup",
    "e2e:v2:seed": "node scripts/run-with-env.js e2e/auth/.env.v2-admin npm run cli -- seed --file e2e/fixtures/seed-v2.json --prefix \"${TEST_PREFIX_V2}\" --json --output e2e/seed-map-v2.json",
    "e2e:v2:cleanup": "node scripts/run-with-env.js e2e/auth/.env.v2-admin npm run cli -- cleanup --prefix \"${TEST_PREFIX_V2}\"",
    "e2e:v2:test": "TEST_VERSION=v2 npx playwright test --config=e2e/playwright.config.ts --project=v2-admin --project=v2-userviewer --project=v2-readonly",
    "e2e:v2:test:admin": "TEST_VERSION=v2 npx playwright test --config=e2e/playwright.config.ts --project=v2-admin",
    "e2e:v2:test:userviewer": "TEST_VERSION=v2 npx playwright test --config=e2e/playwright.config.ts --project=v2-userviewer",
    "e2e:v2:test:readonly": "TEST_VERSION=v2 npx playwright test --config=e2e/playwright.config.ts --project=v2-readonly",
    "e2e:v2": "npm run e2e:v2:seed && npm run e2e:v2:test; npm run e2e:v2:cleanup",
    "e2e:v2:admin": "npm run e2e:v2:seed && npm run e2e:v2:test:admin; npm run e2e:v2:cleanup",
    "e2e:v2:userviewer": "npm run e2e:v2:seed && npm run e2e:v2:test:userviewer; npm run e2e:v2:cleanup",
    "e2e:v2:readonly": "npm run e2e:v2:seed && npm run e2e:v2:test:readonly; npm run e2e:v2:cleanup",
    "e2e:seed": "npm run e2e:v1:seed; npm run e2e:v2:seed",
    "e2e:cleanup": "npm run e2e:v1:cleanup; npm run e2e:v2:cleanup",
    "e2e:test": "npx playwright test --config=e2e/playwright.config.ts --project=v1-admin --project=v1-userviewer --project=v1-readonly --project=v2-admin --project=v2-userviewer --project=v2-readonly",
    "e2e:baseline": "node scripts/check-test-baseline.js",
    "e2e": "npm run e2e:cleanup; npm run e2e:seed && npm run e2e:test; r=$?; npm run e2e:cleanup; exit $r",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src e2e .storybook",
    "lint:js:fix": "eslint config src e2e .storybook --fix",
    "lint:e2e": "tsc --noEmit -p e2e/tsconfig.json",
    "lint:prettier": "prettier --check \"src/**/*.{ts,tsx,js}\" \"e2e/**/*.ts\" \"config/**/*.ts\" \".storybook/**/*.{ts,tsx}\"",
    "lint:permissions": "npx tsx scripts/validate-permissions.ts",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "HOT=true fec dev-proxy",
    "start:no-proxy": "HOT=true fec dev",
    "static": "fec static",
    "translations": "npm-run-all translations:*",
    "translations:extract": "npx formatjs extract ./src/*.js --out-file ./locales/translation-template.json",
    "translations:compile": "npx formatjs compile ./locales/translation-template.json --out-file ./src/locales/translations.json",
    "translations:datafile": "node scripts/createDataJson.js",
    "circular-dependencies": "madge --circular --extensions ts,js ./src",
    "deploy": "npm-run-all build circular-dependencies lint",
    "verify": "npm-run-all build circular-dependencies lint test-storybook:ci",
    "nightly": "npm run deploy",
    "postinstall": "ts-patch install && rm -rf .cache",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook --excludeTags test-skip",
    "test-storybook:fast": "CI=true test-storybook --excludeTags test-skip",
    "test-storybook:ci": "test-storybook --ci --excludeTags test-skip",
    "cli": "node --import tsx src/cli/cli.tsx",
    "test:cli": "vitest run --config src/cli/vitest.config.ts"
  },
  "insights": {
    "appname": "rbac"
  },
  "msw": {
    "workerDirectory": [
      "static"
    ]
  }
}
