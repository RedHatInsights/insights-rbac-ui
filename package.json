{
  "name": "insights-rbac-frontend",
  "version": "1.1.0",
  "private": false,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@babel/compat-data": "^7.17.7",
    "@babel/runtime": "^7.17.7",
    "@babel/types": "^7.17.0",
    "@data-driven-forms/pf4-component-mapper": "^3.16.9",
    "@data-driven-forms/react-form-renderer": "^3.16.9",
    "@formatjs/cli": "5.0.6",
    "@patternfly/quickstarts": "^2.3.1",
    "@patternfly/react-core": "4.198.19",
    "@patternfly/react-icons": "^4.86.7",
    "@patternfly/react-table": "^4.67.19",
    "@patternfly/react-tokens": "^4.87.7",
    "@redhat-cloud-services/access-requests-frontend": "^1.2.11",
    "@redhat-cloud-services/cost-management-client": "^1.0.104",
    "@redhat-cloud-services/frontend-components": "^3.8.10",
    "@redhat-cloud-services/frontend-components-notifications": "^3.2.10",
    "@redhat-cloud-services/frontend-components-utilities": "^3.2.24",
    "@redhat-cloud-services/rbac-client": "^1.0.110",
    "axios-mock-adapter": "^1.20.0",
    "babel-loader": "^8.2.3",
    "classnames": "^2.3.1",
    "file-loader": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^6.0.5",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.4.1"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/stories/*"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/entry.js",
      "<rootDir>/src/entry-dev.js",
      "<rootDir>/src/logout.js"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.2.2",
    "@redhat-cloud-services/frontend-components-config": "^4.6.22",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^28.1.3",
    "babel-plugin-dual-import": "^1.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "cross-fetch": "^3.1.5",
    "css-loader": "^5.2.6",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.1",
    "ts-jest": "^28.0.6",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "analyze": "NODE_ENV=production webpack --config config/prod.webpack.config.js --env analyze=true",
    "test": "TZ=UTC jest --verbose -i--no-cache",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "prod": "NODE_ENV=production webpack serve --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "start:beta": "NODE_ENV=development BETA=true webpack serve --config config/dev.webpack.config.js",
    "start:proxy": "PROXY=true npm run start",
    "start:proxy:beta": "BETA=true npm run start:proxy",
    "translations": "npm-run-all translations:*",
    "translations:extract": "npx formatjs extract ./src/*.js --out-file ./locales/translation-template.json",
    "translations:compile": "npx formatjs compile ./locales/translation-template.json --out-file ./src/locales/translations.json",
    "translations:datafile": "node scripts/createDataJson.js",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build:prod",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run deploy"
  },
  "insights": {
    "appname": "rbac"
  }
}
