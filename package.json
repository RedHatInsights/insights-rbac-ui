    "classnames": "^2.3.2",
    "file-loader": "^6.2.0",
    "glob": "^11.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.5.2",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.3",
    "redux-thunk": "^2.4.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/stories/*"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/entry.js",
      "<rootDir>/src/entry-dev.js",
      "<rootDir>/src/logout.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.23.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-flow": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@cypress/code-coverage": "^3.14.4",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^3.0.0",
    "@redhat-cloud-services/frontend-components-config": "^6.6.0",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@storybook/addon-docs": "^9.0.12",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/react-webpack5": "^9.0.12",
    "@storybook/test-runner": "^0.23.0",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "axios-mock-adapter": "^1.22.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-dual-import": "^1.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "chokidar": "^4.0.3",
    "chromatic": "^13.0.1",
    "cross-fetch": "^4.0.0",
    "css-loader": "^6.8.1",
    "cypress": "^13.6.1",
    "cypress-localstorage-commands": "^2.2.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.0.12",
    "eslint-plugin-testing-library": "^7.6.3",
    "husky": "^9.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "madge": "^8.0.0",
    "msw": "^2.10.4",
    "msw-storybook-addon": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.1",
    "redux-mock-store": "^1.5.4",
    "resolve-url-loader": "^5.0.0",
    "semantic-release": "^24.1.2",
    "storybook": "^9.0.12",
    "ts-jest": "^29.1.1",
    "ts-patch": "^3.0.2",
    "typescript": "^5.2.2",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-dev-server": "^4.15.1"
  },
  "scripts": {
    "prepare": "husky",
    "build": "fec build",
    "cypress": "cypress",
    "cypress:run": "cypress run --browser electron",
    "test": "TZ=UTC jest --verbose",
    "test:e2e": "node scripts/e2e.js",
    "test:ct": "node ./cypress/run-test.js",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src cypress",
    "lint:js:fix": "eslint config src cypress --fix",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "HOT=true fec dev",
    "translations": "npm-run-all translations:*",
    "translations:extract": "npx formatjs extract ./src/*.js --out-file ./locales/translation-template.json",
    "translations:compile": "npx formatjs compile ./locales/translation-template.json --out-file ./src/locales/translations.json",
    "translations:datafile": "node scripts/createDataJson.js",
    "circular-dependencies": "madge --circular --extensions ts,js ./src",
    "deploy": "npm-run-all build circular-dependencies lint test",
    "verify": "npm-run-all build circular-dependencies lint test test-storybook:ci",
    "nightly": "npm run deploy",
    "postinstall": "ts-patch install && rm -rf .cache",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "test-storybook --ci"
  },
  "insights": {
    "appname": "rbac"
  },
  "msw": {
    "workerDirectory": [
      "static"
    ]
  }
}
